{
  "cli": {
    "version": ">= 5.9.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development",
        "IOS_BUNDLE_ID": "io.daymoon.rec.dev",
        "ANDROID_PACKAGE": "io.daymoon.rec.dev"
      }
    },
    "production": {
      "environment": "production",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production",
        "IOS_BUNDLE_ID": "io.daymoon.rec",
        "ANDROID_PACKAGE": "io.daymoon.rec"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
