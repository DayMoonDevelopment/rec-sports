# Mobile App Environment Variables

# Configuration
APP_ENV=development
IOS_BUNDLE_ID=your.bundle.identifier
ANDROID_PACKAGE=your.android.package

# Google Authentication & Maps
GOOGLE_MAPS_ANDROID_SDK_API_KEY=your_google_maps_android_api_key_here
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here
GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here

# Data
EXPO_PUBLIC_GQL_DATA_URL=http://localhost:4000/graphql

# Algolia Search
EXPO_PUBLIC_ALGOLIA_APP_ID=your_algolia_app_id_here
EXPO_PUBLIC_ALGOLIA_SEARCH_API_KEY=your_algolia_search_api_key_here
EXPO_PUBLIC_ALGOLIA_INDEX_NAME=locations