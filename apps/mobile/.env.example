# GraphQL API endpoint for the mobile app
EXPO_PUBLIC_GQL_DATA_URL=https://your-api-endpoint.com/graphql

# Google Maps SDK for Android
GOOGLE_MAPS_ANDROID_SDK_API_KEY=your_google_maps_android_api_key_here

# App bundle identifiers (set by EAS build profiles)
IOS_BUNDLE_ID=your.bundle.identifier
ANDROID_PACKAGE=your.android.package

# App environment (development/production)
APP_ENV=development